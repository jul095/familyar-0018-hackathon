{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "lib": ["dom", "es6", "es2017", "esnext.asynciterable"],
    "sourceMap": true,
    "outDir": "./dist",
    "types": ["reflect-metadata"],
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowJs": true,
    "baseUrl": "."
  },
  "exclude": ["node_modules", "dist", "dist-web"],
  "include": ["./src/**/*.tsx", "./src/**/*.ts"],
  "references": []
}
